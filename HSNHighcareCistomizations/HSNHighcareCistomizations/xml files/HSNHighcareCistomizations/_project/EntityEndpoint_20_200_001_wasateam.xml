                        <To object="EquipmentSelected: 3" field="ManufacturerID" />
                    </Mapping>
                </Mapping>
                <Mapping field="Location">
                    <To object="EquipmentRecords" field="CustomerLocationID" />
                </Mapping>
                <Mapping field="LocationType">
